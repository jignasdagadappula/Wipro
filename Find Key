class FindKey 
{
	public static void main(String[] args) 
	{
		int input1=3521;int input2=2452;int input3=1352;
		int largest=0,rem=0;
		int a=input1/100;
		int b=a%10;
		int c=input2/10;
		int d=c%10;
		while(input3>0){
			rem=input3%10;
			if(largest<rem)
				largest=rem;
			input3/=10;
		}
		int e=(b*d)*largest;
		System.out.println(e);
	}
}
